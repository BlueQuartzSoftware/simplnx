{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "complex",
  "version": "0.1.0",
  "dependencies": [
    {
      "name": "fmt"
    },
    {
      "name": "nlohmann-json"
    },
    {
      "name": "expected-lite"
    },
    {
      "name": "eigen3"
    },
    {
      "name": "hdf5",
      "features": [
        "cpp"
      ]
    },
    {
      "name": "span-lite"
    },
    {
      "name": "boost-mp11"
    },
    {
      "name": "nod"
    },
    {
      "name": "reproc"
    }
  ],
  "features": {
    "python": {
      "description": "Python bindings",
      "dependencies": [
        {
          "name": "pybind11",
          "version>=": "2.6.2"
        }
      ]
    },
    "tests": {
      "description": "Tests",
      "dependencies": [
        {
          "name": "catch2"
        }
      ]
    },
    "parallel": {
      "description": "Parallel support with TBB",
      "dependencies": [
        {
          "name": "tbb"
        }
      ]
    },
    "itk": {
      "description": "ITK",
      "dependencies": [
        {
          "name": "itk"
        }
      ]
    },
    "ebsd": {
      "description": "Ebsd data processing",
      "dependencies": [
        {
          "name": "ebsdlib"
        },
        {
          "name": "h5support"
        },
        {
          "name": "tbb"
        }
      ]
    },
    "compressors": {
      "description": "Compressors for binary data",
      "dependencies": [
        {
          "name": "blosc"
        }
      ]
    }
  }
}
